# common Khadas sbc overlay config

CONFIG_AUTOBOOT=y
CONFIG_AUTOBOOT_KEYED=y
CONFIG_AUTOBOOT_PROMPT="Hit SPACE in %d seconds to stop autoboot"
# CONFIG_AUTOBOOT_ENCRYPTION is not set
CONFIG_AUTOBOOT_DELAY_STR="2"
CONFIG_AUTOBOOT_STOP_STR=" "
CONFIG_AUTOBOOT_KEYED_CTRLC=y

CONFIG_NETCONSOLE=y

#CONFIG_SILENT_CONSOLE=y
## CONFIG_SILENT_U_BOOT_ONLY is not set
#CONFIG_SILENT_CONSOLE_UPDATE_ON_SET=y
## CONFIG_SILENT_CONSOLE_UPDATE_ON_RELOC is not set

CONFIG_CMD_CLS=y
# logo deps
CONFIG_USE_PREBOOT=y

CONFIG_CMD_NVEDIT_EFI=y

CONFIG_SYSINFO=y
CONFIG_SYSINFO_SMBIOS=y

## SPI default
CONFIG_SF_DEFAULT_MODE=0
CONFIG_SF_DEFAULT_SPEED=0
