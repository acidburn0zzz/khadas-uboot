# Edge overlay config

# fusb302
CONFIG_POWER_FUSB302=y

# spi flash

# CONFIG_TPL is not set
CONFIG_SPI_FLASH_WINBOND=y
CONFIG_ROCKCHIP_SPI=y
CONFIG_SF_DEFAULT_SPEED=104000000
CONFIG_SPL_SPI_FLASH_SUPPORT=y
CONFIG_SPL_SPI_SUPPORT=y
CONFIG_SPL_SPI_LOAD=y
CONFIG_SPL_TEXT_BASE=0xff8c2000
CONFIG_SYS_MMCSD_RAW_MODE_U_BOOT_SECTOR=0x200
CONFIG_SYS_SPI_U_BOOT_OFFS=0x40000

CONFIG_ENV_SPI_BUS=1
CONFIG_SF_DEFAULT_BUS=1

# video
CONFIG_DISPLAY=y
#CONFIG_DISPLAY_BOARDINFO_LATE=y
CONFIG_VIDEO_ROCKCHIP=y
CONFIG_DISPLAY_ROCKCHIP_HDMI=y

# reset
CONFIG_DM_RESET=y
CONFIG_RESET_ROCKCHIP=y

# nvme pci
CONFIG_CMD_PCI=y
CONFIG_PCI=y
CONFIG_NVME=y

# usb
CONFIG_PHY_ROCKCHIP_INNO_USB2=y
CONFIG_PHY_ROCKCHIP_TYPEC=y
CONFIG_USB_XHCI_ROCKCHIP=y
CONFIG_USB_KEYBOARD=y
CONFIG_USB_XHCI_DWC3=y
CONFIG_USB_DWC3=y
CONFIG_USB_DWC3_GENERIC=y
CONFIG_ROCKCHIP_USB2_PHY=y
CONFIG_USB_OHCI_HCD=y
CONFIG_USB_OHCI_GENERIC=y

# net
# CONFIG_NET_RANDOM_ETHADDR is not set
CONFIG_MISC_INIT_R=y
CONFIG_MISC=y
CONFIG_ROCKCHIP_EFUSE=y
# CONFIG_SPL_DRIVERS_MISC_SUPPORT is not set

#CONFIG_ENV_OFFSET=0x145000
CONFIG_ENV_OFFSET=0x150000

# size optimize
#CONFIG_SPL_ZSTD=y
